        <TextBox HorizontalAlignment="Left" Margin="55,195,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="173" Height="35" Background="{x:Null}" BorderBrush="#FFF7773D" BorderThickness="2,2,2,2"/>
        <TextBox HorizontalAlignment="Left" Margin="55,88,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="173" Height="35" Background="{x:Null}" BorderBrush="#FFF7773D" BorderThickness="2,2,2,2"/>
        <TextBox HorizontalAlignment="Left" Margin="287,88,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="173" Height="35" Background="{x:Null}" BorderBrush="#FFF7773D" BorderThickness="2,2,2,2"/>

    </Grid>
