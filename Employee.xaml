        <TextBox HorizontalAlignment="Left" Margin="287,195,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="173" Height="35" Background="{x:Null}" BorderBrush="#FFF7773D" BorderThickness="2,2,2,2"/>
        <Button Content="ADD" HorizontalAlignment="Left" Height="38" Margin="302,256,0,0" VerticalAlignment="Top" Width="72" Background="#FFF7773D" Foreground="White"/>
